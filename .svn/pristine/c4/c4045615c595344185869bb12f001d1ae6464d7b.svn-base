<UserControl x:Class="QLNH.ThietKe.iDesign"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:ThietKe="clr-namespace:QLNH.ThietKe"
             d:DesignHeight="600"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <Grid x:Name="LayoutRoot">
        <Rectangle Fill="White" Stroke="Black" />
        <Grid Margin="5,5,5,5">
            <Grid x:Name="GridDesign" Margin="0">
                <ThietKe:iEnvironment x:Name="MC" />
                <!--  <Label Content="Label" Height="26" HorizontalAlignment="Left" Margin="6,6,0,0" Name="label1" VerticalAlignment="Top" Width="88" />  -->
            </Grid>
        </Grid>
    </Grid>
</UserControl>
